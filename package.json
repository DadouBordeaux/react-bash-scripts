{
  "name": "react-generator",
  "version": "0.0.8",
  "description": "Crossplatform Node CLI to generate and automate common tasks in react. Support module generateion, components, containers, reducers, test. Easily configurable to your own needs.",
  "repository": "https://github.com/JuliusKoronci/react-bash-scripts",
  "main": "./build/index.js",
  "scripts": {
    "gitCommit": "git add . && git commit -m'auto commit npm publish/version update script' --allow-empty",
    "gitPush": "git push",
    "babelCompile": "babel src --out-dir build --source-maps",
    "minorVersion": "npm version patch",
    "test": "jest",
    "flow": "flow",
    "build": "npm run gitCommit &&  npm run minorVersion"
  },
  "keywords": [
    "Code Generator",
    "Node CLI",
    "ReactJs"
  ],
  "author": "Julius Koronci <jk@web-solutions.sk>",
  "license": "MIT",
  "bin": {
    "react-cli": "./build/index.js"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "chalk": "^1.1.3",
    "co": "^4.6.0",
    "co-prompt": "^1.0.0",
    "commander": "^2.9.0",
    "figlet": "^1.2.0",
    "flow-bin": "^0.39.0",
    "fs": "^0.0.1-security",
    "is-empty": "^1.2.0",
    "jest": "^19.0.2",
    "keymirror": "^0.1.1",
    "ramda": "0.23.x",
    "react": "^15.4.2",
    "redux-actions": "^2.0.1",
    "shelljs": "^0.7.7",
    "updeep": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "regenerator-runtime": "^0.10.3",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "redux-mock-store": "^1.2.2",
    "skin-deep": "^1.0.0-alpha2",
    "enzyme": "^2.7.1",
    "enzyme-to-json": "^1.5.0"
  }
}
